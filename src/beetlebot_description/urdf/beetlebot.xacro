<?xml version='1.0' encoding='utf-8'?>
<robot name="beetlebot" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:arg name="namespace" default="" />
    <xacro:property name="namespace" value="$(arg namespace)" />

    <xacro:include filename="$(find beetlebot_description)/urdf/beetlebot.gazebo" />

    <material name="base_link_material">
        <color rgba="0.033104766565152086 0.46778379610254284 0.12477181755144427 1" />
    </material>
    <material name="wheel_right_material">
        <color rgba="0.6375968739867731 0.04373502925049377 0.008023192982520563 1" />
    </material>
    <material name="wheel_left_material">
        <color rgba="0.6375968739867731 0.04373502925049377 0.008023192982520563 1" />
    </material>
    <material name="top_base_material">
        <color rgba="0.033104766565152086 0.46778379610254284 0.12477181755144427 1" />
    </material>
    <material name="front_caster_material">
        <color rgba="0.10702310296918527 0.0069954101845983935 0.3371636150376657 1" />
    </material>

    <material name="rear_caster_material">
        <color rgba="0.10702310296918527 0.0069954101845983935 0.3371636150376657 1" />
    </material>

    <material name="right_bar_material">
        <color rgba="0.23839757380151394 0.12743768042608497 0.7529422167708612 1" />
    </material>

    <material name="left_bar_material">
        <color rgba="0.23839757380151394 0.12743768042608497 0.7529422167708612 1" />
    </material>
    <material name="rear_bar_material">
        <color rgba="0.23839757380151394 0.12743768042608497 0.7529422167708612 1" />
    </material>

    <material name="lidar_material">
        <color rgba="0 0 0 1" />
    </material>

    <material name="camera_material">
        <color rgba="0.015996293361446288 0.22696587349938613 0.07036009568874305 1" />
    </material>

    <material name="imu_material">
        <color rgba="0.25 0.33 0.33 1" />
    </material>

    <!-- Base Link -->
    <link name="${namespace}base_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.3" length="0.1" />
            </geometry>
            <material name="base_link_material" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.3" length="0.1" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0.3" rpy="0 0 0" />
            <mass value="5" />
            <inertia ixx="1.5417" ixy="0" ixz="0" iyy="3.467" iyz="0" izz="4.742" />
        </inertial>
    </link>

    <!-- Right Wheel Link -->
    <link name="${namespace}wheel_right">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.15" length="0.07" />
            </geometry>
            <material name="wheel_right_material" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.15" length="0.07" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0"
                izz="0.005" />
        </inertial>
    </link>

    <!-- Right Wheel Joint -->
    <joint name="${namespace}base_link_to_wheel_right" type="continuous">
        <parent link="${namespace}base_link" />
        <child link="${namespace}wheel_right" />
        <origin xyz="0 -0.33 -0.05" rpy="1.5707963267948963 0 0" />
        <axis xyz="0 0 -1" />
    </joint>

    <!-- Left Wheel Link -->
    <link name="${namespace}wheel_left">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.15" length="0.07" />
            </geometry>
            <material name="wheel_left_material" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.15" length="0.07" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0"
                izz="0.005" />
        </inertial>
    </link>

    <!-- Left Wheel Joint -->
    <joint name="${namespace}base_link_to_wheel_left" type="continuous">
        <parent link="${namespace}base_link" />
        <child link="${namespace}wheel_left" />
        <origin xyz="0 0.33 -0.05" rpy="1.5707963267948963 0 0" />
        <axis xyz="0 0 -1" />
    </joint>

    <!-- Top Base Link -->
    <link name="${namespace}top_base">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.3" length="0.1" />
            </geometry>
            <material name="top_base_material" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.3" length="0.1" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0"
                izz="0.005" />
        </inertial>
    </link>

    <!-- Top Base Joint -->
    <joint name="${namespace}base_link_to_top_base" type="fixed">
        <parent link="${namespace}base_link" />
        <child link="${namespace}top_base" />
        <origin xyz="0 0 0.45" rpy="0 0 0" />
    </joint>

    <!-- Front Caster Link -->
    <link name="${namespace}front_caster">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <sphere radius="0.1" />
            </geometry>
            <material name="front_caster_material" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <sphere radius="0.1" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0"
                izz="0.005" />
        </inertial>
    </link>

    <!-- Front Caster Joint -->
    <joint name="${namespace}base_link_to_front_caster" type="fixed">
        <parent link="${namespace}base_link" />
        <child link="${namespace}front_caster" />
        <origin xyz="0.2 0 -0.1" rpy="0 0 0" />
        <axis xyz="0 1 0" />
    </joint>

    <!-- Rear Caster Link -->
    <link name="${namespace}rear_caster">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <sphere radius="0.1" />
            </geometry>
            <material name="rear_caster_material" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <sphere radius="0.1" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.4" ixy="0" ixz="0" iyy="0.4" iyz="0" izz="0.4" />
        </inertial>
    </link>

    <!-- Rear Caster Joint -->
    <joint name="${namespace}base_link_to_rear_caster" type="fixed">
        <parent link="${namespace}base_link" />
        <child link="${namespace}rear_caster" />
        <origin xyz="-0.2 0 -0.1" rpy="0 0 0" />
        <axis xyz="0 1 0" />
    </joint>

    <!-- Right Bar Link -->
    <link name="${namespace}right_bar">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.03" length="0.5" />
            </geometry>
            <material name="right_bar_material" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.03" length="0.5" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.3333333333333333" ixy="0" ixz="0" iyy="0.5" iyz="0"
                izz="0.3333333333333333" />
        </inertial>
    </link>

    <!-- Right Bar Joint -->
    <joint name="${namespace}base_link_to_right_bar" type="fixed">
        <parent link="${namespace}base_link" />
        <child link="${namespace}right_bar" />
        <origin xyz="0 0.2 0.2058808723746403" rpy="0 0 0" />
    </joint>

    <!-- Left Bar Link -->
    <link name="${namespace}left_bar">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.03" length="0.5" />
            </geometry>
            <material name="left_bar_material" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.03" length="0.5" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.3333333333333333" ixy="0" ixz="0" iyy="0.5" iyz="0"
                izz="0.3333333333333333" />
        </inertial>
    </link>

    <!-- Left Bar Joint -->
    <joint name="${namespace}base_link_to_left_bar" type="fixed">
        <parent link="${namespace}base_link" />
        <child link="${namespace}left_bar" />
        <origin xyz="0 -0.2 0.2058808723746403" rpy="0 0 0" />
    </joint>

    <!-- Rear Bar Link -->
    <link name="${namespace}rear_bar">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.03" length="0.5" />
            </geometry>
            <material name="rear_bar_material" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.03" length="0.5" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.3333333333333333" ixy="0" ixz="0" iyy="0.5" iyz="0"
                izz="0.3333333333333333" />
        </inertial>
    </link>

    <!-- Rear Bar Joint -->
    <joint name="${namespace}base_link_to_rear_bar" type="fixed">
        <parent link="${namespace}base_link" />
        <child link="${namespace}rear_bar" />
        <origin xyz="-0.2 0 0.2058808723746403" rpy="0 0 0" />
    </joint>

    <!-- Lidar Link -->
    <link name="${namespace}lidar">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.1" length="0.13538975482266932" />
            </geometry>
            <material name="lidar_material" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.1" length="0.13538975482266932" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.3333333333333333" ixy="0" ixz="0" iyy="0.5" iyz="0"
                izz="0.3333333333333333" />
        </inertial>
    </link>

    <!-- Lidar Joint -->
    <joint name="${namespace}base_link_to_lidar" type="fixed">
        <parent link="${namespace}base_link" />
        <child link="${namespace}lidar" />
        <origin xyz="0 0 0.57" rpy="0 0 0" />
    </joint>

    <!-- Camera Link -->
    <link name="${namespace}camera">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <box size="0.05 0.2 0.1" />
            </geometry>
            <material name="camera_material" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <box size="0.05 0.2 0.1" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.16666666666666666" ixy="0" ixz="0" iyy="0.16666666666666666" iyz="0"
                izz="0.16666666666666666" />
        </inertial>
    </link>

    <!-- Camera Joint -->
    <joint name="${namespace}base_link_to_camera" type="revolute">
        <parent link="${namespace}base_link" />
        <child link="${namespace}camera" />
        <origin xyz="0.3 0 0.34" rpy="0 0 0" />
        <axis xyz="0 1 0" />

        <limit lower="-1.14" upper="1.14" velocity="3.14" effort="10.0" />
    </joint>

    <link name="${namespace}imu_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.05" length="0.135" />
            </geometry>
            <material name="imu_material" />
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.075" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.333" ixy="0" ixz="0" iyy="0.5" iyz="0" izz="0.333" />
        </inertial>
    </link>


    <joint name="${namespace}base_link_to_imu" type="fixed">
        <parent link="${namespace}base_link" />
        <child link="${namespace}imu_link" />
        <origin xyz="0 0 0.1" rpy="0 0 0" />
    </joint>

    <gazebo reference="${namespace}imu_link">
        <sensor name="imu_sensor" type="imu">
            <ignition_frame_id>imu_link</ignition_frame_id>
            <plugin filename="ignition-gazebo-imu-system" name="ignition::gazebo::systems::Imu">
            </plugin>
            <topic>imu</topic>
            <always_on>true</always_on>
            <update_rate>100</update_rate>
            <visualize>true</visualize>
            <imu>
                <angular_velocity>
                    <x>
                        <noise type="gaussian">
                            <mean>0.0</mean>
                            <stddev>2e-4</stddev>
                            <bias_mean>0.0000075</bias_mean>
                            <bias_stddev>0.0000008</bias_stddev>
                        </noise>
                    </x>
                    <y>
                        <noise type="gaussian">
                            <mean>0.0</mean>
                            <stddev>2e-4</stddev>
                            <bias_mean>0.0000075</bias_mean>
                            <bias_stddev>0.0000008</bias_stddev>
                        </noise>
                    </y>
                    <z>
                        <noise type="gaussian">
                            <mean>0.0</mean>
                            <stddev>2e-4</stddev>
                            <bias_mean>0.0000075</bias_mean>
                            <bias_stddev>0.0000008</bias_stddev>
                        </noise>
                    </z>
                </angular_velocity>
                <linear_acceleration>
                    <x>
                        <noise type="gaussian">
                            <mean>0.0</mean>
                            <stddev>1.7e-3</stddev>
                            <bias_mean>0.01</bias_mean>
                            <bias_stddev>0.001</bias_stddev>
                        </noise>
                    </x>
                    <y>
                        <noise type="gaussian">
                            <mean>0.0</mean>
                            <stddev>1.7e-3</stddev>
                            <bias_mean>0.01</bias_mean>
                            <bias_stddev>0.001</bias_stddev>
                        </noise>
                    </y>
                    <z>
                        <noise type="gaussian">
                            <mean>0.0</mean>
                            <stddev>1.7e-3</stddev>
                            <bias_mean>0.01</bias_mean>
                            <bias_stddev>0.001</bias_stddev>
                        </noise>
                    </z>
                </linear_acceleration>
            </imu>
        </sensor>
    </gazebo>
</robot>